<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">
<Script file="CDMLoaderUIButtonMixin.lua"/>
<Button name="CDMTabButtonTemplate" virtual="true" mixin="CDMLoaderUIButtonMixin">
  <Size x="43" y="55"/>
  <Layers>
    <Layer level="BACKGROUND">
      <Texture parentKey="Background" atlas="questlog-tab-side" useAtlasSize="true">
        <Anchors>
          <Anchor point="CENTER"/>
        </Anchors>
      </Texture>
    </Layer>

    <Layer level="ARTWORK">
      <Texture parentKey="Icon">
        <Size x="36" y="36"/>
        <Anchors>
          <Anchor point="CENTER" x="-2"/>
        </Anchors>
      </Texture>
    </Layer>

    <Layer level="HIGHLIGHT">
      <Texture atlas="QuestLog-Tab-side-Glow-hover" useAtlasSize="true">
        <Anchors>
          <Anchor point="CENTER"/>
        </Anchors>
      </Texture>
    </Layer>
  </Layers>

  <Scripts>
    <OnEnter method="OnEnter"/>
    <OnMouseDown method="OnMouseDown"/>
    <OnMouseUp method="OnMouseUp"/>
  </Scripts>
</Button>

</Ui>

